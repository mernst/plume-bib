@Article{Bjorner:1997:AGI,
  author =       "Nicolaj Bj{\o}rner and Anca Browne and Zohar Manna",
  title =        "Automatic generation of invariants and intermediate
                 assertions",
  journal =      "Theoretical Computer Science",
  volume =       "173",
  number =       "1",
  pages =        "49--87",
  day =          "20",
