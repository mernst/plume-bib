@TechReport{ncstrl.ucsb_cs//TRCS98-31,
  type =         "Technical Report",
  number =       "TRCS98-31",
  institution =  "University of California, Santa Barbara",
  title =        "{jContractor}: A Reflective {Java} Library to Support
                 Design by Contract",
  month =        jan # "~19,",
