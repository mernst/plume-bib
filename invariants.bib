@Article{HuismanJvdB2001,
  author = 	 "Marieke Huisman and Bart P.F. Jacobs and Joachim A.G.M. {van den Berg}",
  title = 	 "A case study in class library verification: {Java}'s {Vector} class",
  journal = 	 STTT,
